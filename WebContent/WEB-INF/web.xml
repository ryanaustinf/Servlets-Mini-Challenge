<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Servlets-Mini-Challenge</display-name>
  <welcome-file-list>
    <welcome-file>start</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Start</servlet-name>
    <servlet-class>com.mc.controller.StartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Start</servlet-name>
    <url-pattern>/start</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Preferences</servlet-name>
    <servlet-class>com.mc.controller.PreferenceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Preferences</servlet-name>
    <url-pattern>/pref</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Posts</servlet-name>
    <servlet-class>com.mc.controller.PostsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Posts</servlet-name>
    <url-pattern>/posts</url-pattern>
  </servlet-mapping>
</web-app>